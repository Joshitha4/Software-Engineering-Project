<%- include('../../partials/header') %>

    <center>
        <div class="container test">
        <form method="POST" action="/create_codingtest/<%=jobId%>">
            <h3>Set Problem</h3>
            <div class="form-group">
                <input type="text" id="name" name="name" class="form-control" placeholder="Problem Name">
            </div>
            <div class="form-group">
                <textarea id="description" name="description" class="form-control" placeholder="Description"></textarea>
            </div>
            <div class="form-group">
                <textarea id="testcases" name="testcases" class="form-control" placeholder="Write Your Testcases"></textarea>
            </div>
            <div class="form-group">
                <textarea id="expectedoutput" name="expectedoutput" class="form-control" placeholder="Expected Output"></textarea>
            </div>
            <button type="submit" class="btn btn-dark">Set question</button>
        </form>
    </div>
    </center>





    <%- include('../../partials/footer') %>